{
  "name": "frontend",
  "version": "1.0.0",
  "author": "Dirty Bits",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "quicktest": "jest --no-cache",
    "coverage": "jest --coverage --no-cache"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestconfig/jestSetup.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-persist": "^5.7.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.1.2",
    "@types/next": "^2.4.8",
    "@types/next-redux-wrapper": "^1.3.7",
    "@types/react": "^16.0.37",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/redux-form": "^7.0.14",
    "@types/redux-mock-store": "^0.0.13",
    "@types/styled-jsx": "^2.2.2",
    "@zeit/next-typescript": "^0.0.9",
    "cypress": "^1.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.2.2",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.6.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.1"
  }
}
