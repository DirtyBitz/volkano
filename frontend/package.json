{
  "name": "frontend",
  "version": "1.0.0",
  "author": "Dirty Bits",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"tsc --pretty --watch\" \"next\"",
    "prebuild": "tsc",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "quicktest": "jest"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestconfig/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jestconfig/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "next": "^4.2.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.7",
    "@types/jest": "^22.0.1",
    "@types/next": "^2.4.5",
    "@types/react": "^16.0.22",
    "@types/styled-jsx": "^2.2.1",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/react-dom": "^16.0.3",
    "enzyme-adapter-react-16": "^1.1.1",
    "concurrently": "^3.5.0",
    "enzyme": "^3.3.0",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.6.2",
    "typescript": "^2.6.1"
  }
}
